    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var time_tk = hour*10000+minute*100+second;

var ampm = &quot;?&quot;;
var hour12 = -1;

if (hour==0 &amp;&amp; minute==0 &amp;&amp; second==0) {
 ampm = &quot;midnight&quot;;
 hour12 = 12;
} else
if (hour==12 &amp;&amp; minute==0 &amp;&amp; second==0) {
 ampm = &quot;noon&quot;;
 hour12 = 12;
} else if (hour==0) { 
 ampm = &quot;am&quot;;
 hour12 = 12; 
} else if (hour==12) {
 ampm = &quot;pm&quot;;
 hour12 = 12;
} else if (hour&gt;=0 &amp;&amp; hour&lt;12) {
 ampm = &quot;am&quot;;
 hour12 = hour;
} else if (hour&gt;=12) {
 ampm = &quot;pm&quot;;
 hour12 = hour - 12;
}

var hms = num2str(hour, &quot;00&quot;)+&quot;:&quot;+num2str(minute, &quot;00&quot;)+&quot;:&quot;+num2str(second, &quot;00&quot;);
var hm = num2str(hour, &quot;00&quot;)+&quot;:&quot;+num2str(minute, &quot;00&quot;);

var hms12 = hour12+&quot;:&quot;+num2str(minute, &quot;00&quot;)+&quot;:&quot;+num2str(second, &quot;00&quot;)+&quot; &quot;+ampm;

</jsScript_script>
